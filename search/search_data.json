---
layout: null
---
[
  {% for post in site.pages %}
  
  {% if page.url contains '.html' %}
  {% unless page.url contains '/blog/' %}
  {% unless page.url == '/search.html' %}
  {% unless page.url == '/search_en.html' %}
  {% unless page.url == '/404.html' %}
  {
    "title": "{{ post.title | escape }}",
    "tags": [{% for tag in post.tags%}"{{ tag }}"{% unless forloop.last %}, {% endunless %}{% endfor %}],
    "url": "{{ post.url }}",
    "content": "{{ post.content | strip | strip_html | strip_newlines | replace, "	", " " | escape  }}"
  }{% unless forloop.last %},{% endunless %}

  {% endunless %}
  {% endunless %}
  {% endunless %}
  {% endunless %}
  {% endif %}
  {% endfor %}
]
