---
layout: null
---
[
{% for post in site.pages %}
  {% if post.url contains '.html' %}
  {% unless post.url == '/index.html' %}
  {% unless post.url == '/404.html' %}
  {
    "title": "{{ post.title | escape }}",
    "url": "{{ post.url }}",
    "content": "{{ post.content | strip | strip_html | strip_newlines | replace, "	", " " | escape  }}"
  }{% unless forloop.last %},{% endunless %}
  {% endunless %}
  {% endunless %}
  {% endif %}
{% endfor %}
  {
    "title": "dummy",
    "url": "/dummy.html",
    "content": "dummy"
  }
]
